
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@model List<E_VillageProject.Models.tbl_signup>
<div class="row">
    <table class="table table-bordered" style="background:linear-gradient(gold,green)">
        <tr>
            <th>Id</th>
            <th>Name</th>
           
            <th>Email</th>
            <th>Password</th>
            <th>Repeat Password</th>
        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.Id</td>
                <td>@x.Your_Name</td>
   
                <td>@x.Your_email</td>
                <td>@x.password</td>
                <td>@x.Repeat_password</td>
            </tr>
        }
    </table>
</div>