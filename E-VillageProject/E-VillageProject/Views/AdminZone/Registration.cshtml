
@{
    ViewBag.Title = "Registration";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model List<E_VillageProject.Models.tbl_registration>
<div class="row">
    <table class="table table-bordered" style="background:linear-gradient(gold,green)">
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Mother's Name</th>
            <th>Father's Name</th>
            <th>Address</th>
            <th>Permanent Address</th>
            <th>Gender</th>
            <th>State</th>
            <th>City</th>
            <th>DOB</th>
            <th>Contact No</th>
            <th>Email</th>
            <th>Profile</th>
            <th>PassWord</th>
            <th> Confirm    PassWord</th>
            <th>Contact Date</th>
            <th>Rge Date</th>
            <th>Insert</th>
            <th>Delete</th>
            <th>Update</th>
        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.Id</td>

                <td>@x.First_name</td>
                <td>@x.Last_name</td>
                <td>@x.Mothers_name</td>
                <td>@x.Father_name</td>
                <td>@x.Address</td>
                <td>@x.Permanent_Address</td>
                <td>@x.Gender</td>
                <td>@x.State</td>
                <td>@x.City</td>
                <td>@x.DOB</td>
                <td>@x.Contactno</td>
                <td>@x.EmailID</td>
                @*<td><img src="~/Content/images/@x.Profile" height="50" width="50" /></td>*@
                <td><img src="~/Content/images/@x.Profile" height="50" width="50" /></td>
                <td>@x.Password</td>
                <td>@x.ConfirmPassword</td>
                <td>@x.Regdate</td>
                
                <td><a href=""><span style="font-size:40px;color:green" class="fa fa-plus"></span></a></td>
                <td><a href="/AdminZone/Delete?m=@x.EmailID"><span style="font-size:40px;color:red" class="fa fa-trash"></span></a></td>
                <td><a href="/AdminZone/UpdateRecord?U=@x.EmailID"><span style="font-size:40px;color:green" class="fa fa-edit"></span></a></td>
               
            </tr>
        }
    </table>
</div>

